var apiKey="RspQyE7vqwceJDz1q5aKkxcFofHac7xmDg5oe3G2";
var labelRootURL="https://api.fda.gov/drug/label.json";
$(document).ready(function()
 {
    $.ajax(
    {
        url: labelRootURL + '?api_key=' + apiKey + '&search=drug_interactions:"aspirin"'
    }).done(function(data) 
    {
    alert (data.results[0].openfda.substance_name[0]);
       $('.drug-name').append(data.results[0].openfda.substance_name[0]);
       $('.interactions').append(data.results[0].boxed_warning[0]);
    });
})

$("#search_input").keypress(function(e) {
  if(e.keyCode == 13)
     {
         e.preventDefault();
         sendSelected(this.value);
         $(this).autocomplete('close');
     }
});