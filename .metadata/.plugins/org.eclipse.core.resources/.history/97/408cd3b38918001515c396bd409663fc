var apiKey="RspQyE7vqwceJDz1";

var labelRootURL="https://api.fda.gov/drug/label.json";
$(document).ready(function()
 {
    
    alert('hi');
    $.ajax(
    {
        url: labelRootURL + '?api_key=' + apiKey + '&search=drug_interactions:"aspirin"';
    }).then(function(data) 
    {
       $('.drug-name').append(data.results[0].openfda.substance[0]);
       $('.interactions').append(data.content.results[0].boxed_warning[0]);
    });
})